# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here

# Model Configuration
MODEL_TYPE=neural_network  # Options: neural_network, ml_classifier, ensemble
SENTIMENT_THRESHOLD_POSITIVE=0.6
SENTIMENT_THRESHOLD_NEGATIVE=0.4

# GA Configuration
GA_POPULATION_SIZE=50
GA_GENERATIONS=20
GA_CROSSOVER_PROBABILITY=0.8
GA_MUTATION_PROBABILITY=0.2

# Processing
MAX_COMMENTS_PER_VIDEO=1000
BATCH_SIZE=32
USE_GPU=true

# Output
EXPORT_FORMAT=pdf  # Options: pdf, csv, json, html
